      /*zone circle 시작*/
      function addZone(map){
          //zone 표시
          for (var i =0; i<zoneArray.length; i++) {
          console.log(zoneArray[i]);
          /*보안구역진입 원1 */ 
          var zoneCircle1 = new google.maps.Circle({
